{
  "name": "pt-coverage-map",
  "description": "Interactive map that shows coverage around public transport routes. Uses offline data (includes script for downloading data).",
  "version": "1.1.0",
  "scripts": {
    "prepare": "npm run build",
    "build": "browserify -p esmify -g browserify-css src/index.js -o dist/map.js",
    "lint": "standard --fix src/*.js",
    "watch": "watchify -p esmify -g browserify-css -d src/index.js -o dist/map.js -v",
    "start": "http-server ./"
  },
  "dependencies": {
    "boundingbox": "^0.1.2",
    "escape-html": "^1.0.3",
    "leaflet": "^1.5.1",
    "leaflet-polylinedecorator": "git+https://github.com/plepe/Leaflet.PolylineDecorator.git",
    "leaflet-polylineoffset": "^1.1.1",
    "overpass-frontend": "^2.0.0",
    "overpass-layer": "^2.0.0",
    "yaml": "^1.9.2"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.12.0",
    "argparse": "^1.0.10",
    "browserify": "^16.2.2",
    "esmify": "^2.1.1",
    "http-server": "^0.12.0",
    "query-string": "^6.9.0",
    "request": "^2.88.0",
    "sheet-router": "^4.2.3",
    "standard": "^12.0.1",
    "watchify": "^3.11.0",
    "xmldom": "^0.1.27"
  },
  "main": "src/index.js",
  "repository": "https://github.com/plepe/pt-coverage-map",
  "author": "Stephan BÃ¶sch-Plepelits <skunk@xover.mud.at>",
  "license": "GPL-3.0"
}
